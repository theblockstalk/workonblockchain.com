    "lint": "ng lint",
    "e2e": "ng e2e",
    "___ANGULAR APP___": "",
    "start": "ng serve",
    "build:prod": "ng run user:build:production",
    "build:staging": "ng run user:build:staging",
    "___SERVER SIDE RENDERING APP___": "",
    "serve:ssr": "node local.js",
    "build:ssr:prod": "npm run build:prod && ng run user:server:production && npm run compile:server",
    "build:ssr:staging": "npm run build:staging && ng run user:server:staging && npm run compile:server",
    "build:ssr:local": "ng run user:build && ng run user:server && npm run compile:server",
    "___SERVERLESS APP___": "",
    "build:serverless:prod:deploy": "npm run build:serverless:prod && serverless deploy --stage production",
    "build:serverless:prod": "ng run user:build:production --base-href /production/ && ng run user:server:production && npm run compile:server",
    "build:serverless:staging:deploy": "npm run build:serverless:staging && serverless deploy --stage staging",
    "build:serverless:staging": "ng run user:build:staging --base-href /staging/ && ng run user:server:staging && npm run compile:server",
    "___COMMON___": "",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "___SERVERLESS APP - !!!NOT USED!!!___": "",
    "deploy": "serverless deploy",
    "build:server:serverless": "ng run user:server && npm run compile:server",
    "build:serverless:deploy2": "npm run build:serverless && npm run deploy",
    "build:serverless": "npm run build:browser:serverless && npm run build:server:serverless",
    "build:browser:serverless": "npm run build:prod --base-href /production/",
    "___!!! SERVER SIDE RENDERING APP - NOT USED!!!___": "",
    "build:prerender": "npm run build:prod2 && node dist/prerender.js",
    "build:prod2": "npm run build:prod && npm run build:server:prod",
    "serve:prerender": "node static.js",
    "build:prod:deploy": "npm run build:prod && npm run deploy",
    "build:server:prod": "ng run user:server && npm run compile:server"
  },
